class Solution(object):
    def countWords(self, words1, words2):
        """
        :type words1: List[str]
        :type words2: List[str]
        :rtype: int
        """
        d={}
        for w in words1:
            if w in d:
                d[w]+=1
            else:
                d[w]=1
        res=0
        for w in words2:
            if w in d and d[w]>1:
                continue
            else:
                if w in d:
                    d[w]-=1
        for k in d:
            if d[k]==0:
                res+=1
        return res

class Solution(object):
    def minimalKSum(self, nums, k):
        """
        :type nums: List[int]
        :type k: int
        :rtype: int
        """
        nums=list(set(nums))
        nums.sort()
        # print(nums)
        n=len(nums)
        sums=0
        for i in range(n):
            if nums[i]<=k:
                k+=1
                sums+=nums[i]
                # print(k,nums[i],sums)
        res=k*(k+1)//2-sums
        return res
        

class Solution(object):
    def unequalTriplets(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        trips = pairs = 0
        count = Counter()
        for i,a in enumerate(nums):
            trips += pairs - count[a] * (i - count[a])
            pairs += i - count[a]
            count[a] += 1
        return trips
        

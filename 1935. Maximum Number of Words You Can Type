class Solution(object):
    def canBeTypedWords(self, text, brokenLetters):
        """
        :type text: str
        :type brokenLetters: str
        :rtype: int
        """
        count=0
        li=text.split()
        for i in range(len(li)):
            temp=0
            for j in range(len(brokenLetters)):
                if brokenLetters[j] in li[i]:
                    temp=1
                    break
            if temp!=1: count+=1
        return count
        

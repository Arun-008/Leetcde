class Solution(object):
    def findWords(self, words):
        """
        :type words: List[str]
        :rtype: List[str]
        """
        row1 = set("qwertyuiop")
        row2 = set("asdfghjkl")
        row3 = set("zxcvbnm")
        li = []
        for word in words:
            lower_word = set(word.lower())  
            if lower_word <= row1 or lower_word <= row2 or lower_word <= row3:
                li.append(word)
        return li

        

class Solution(object):
    def triangularSum(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        n = len(nums)
        coeff = 1
        total = 0
        for i in range(n):
            total += coeff * nums[i]
            coeff = coeff * (n-1-i) // (i+1) if i < n-1 else 1
        return total % 10
                  
